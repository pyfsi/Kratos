{
    "settings" :
    {
        "echo_level" : 1,
        "start_step" : 1,
        "stop_step" : 100,
        "data_structure" : "pyKratos"
    },
    "coupled_solver" :
    {
        "type" : "coupled_solvers.iqni",
        "settings" :
        {
            "echo_level" : 1,
            "omega" : 0.2,
            "model" :
            {
                "type": "coupled_solvers.models.ls",
                "settings":
                {
                    "min_significant": 1e-10,
                    "q": 10
                }
            }
        },
        "predictor" :
        {
            "type" : "predictors.linear"
        },
        "convergence_criterion" :
        {
            "type" : "convergence_criteria.or",
            "settings" :
            {
                "convergence_criterion0" :
                {
                    "type": "convergence_criteria.iteration_limit",
                    "settings":
                    {
                        "maximum": 20
                    }
                },
                "convergence_criterion1" :
                {
                    "type" : "convergence_criteria.relative_norm",
                    "settings" :
                    {
                        "tolerance" : 1e-3,
                        "order" : 2
                    }
                }
            }
        },
        "solver_wrappers" :
        [
            {
                "type": "solver_wrappers.fluent.2019R1",
                "settings": {
                    "working_directory" : "CFD",
                    "case_file": "case_tube2d.cas",
                    "dimensions": 2,
                    "unsteady": true,
                    "timestep_start": 0,
                    "thread_names": ["beamoutside"],
                    "delta_t": 1e-04,
                    "interface_input":
                    {
                        "beamoutside_nodes": "DISPLACEMENT"
                    },
                    "interface_output":
                    {
                        "beamoutside_faces": ["PRESSURE", "TRACTION"]
                    },
                    "cores": 4,
                    "fluent_gui": false,
                    "max_nodes_per_face": 4,
                    "hybrid_initialization": false,
                    "flow_iterations": 200,
                    "save_iterations": 1
                }
            },
            {
                "type": "solver_wrappers.mapped",
                "settings": {
                    "solver_wrapper" :
                    {
                        "type": "solver_wrappers.abaqus.614",
                        "settings": {
                            "working_directory" : "CSM",
                            "cores": 1,
                            "dimensions": 2,
                            "arraysize": 10e5,
                            "surfaces": 1,
                            "ramp": 0,
                            "delta_T": 1e-4,
                            "timestep_start": 0,
                            "surfaceIDs": ["BEAMINSIDEMOVING"],
                            "interface_input":
                            {
                                "BEAMINSIDEMOVING_load_points": ["PRESSURE", "TRACTION"]
                            },
                            "interface_output":
                            {
                                "BEAMINSIDEMOVING_nodes": "DISPLACEMENT"
                            },
                            "mp_mode": "THREADS",
                            "input_file": "Base.inp",
                            "save_iterations": 1
                        }
                    },
                    "mapper_interface_input":
                    {
                        "type" : "mappers.interface",
						"settings" :
						{
	                        "type" : "mappers.combined",
                            "settings" :
                            {
                                "mappers" :
                                [
                                    {
                                        "type" :   "mappers.permutation",
                                        "settings" :
                                        {
                                            "permutation" : [1, 0, 2]
                                        }
                                    },
                                    {
                                        "type": "mappers.linear",
                                        "settings":
                                        {
                                            "directions" : ["X","Y"],
                                            "balanced_tree" : false,
                                            "parallel": true
                                        }
                                    }
                                ]
                            }
						}
                    },
                    "mapper_interface_output":
                    {
                        "type" : "mappers.interface",
						"settings" :
                        {
                            "type": "mappers.combined",
                            "settings": {
                                "mappers": [
                                    {
                                        "type": "mappers.permutation",
                                        "settings": {
                                            "permutation": [
                                                1,
                                                0,
                                                2
                                            ]
                                        }
                                    },
                                    {
                                        "type": "mappers.linear",
                                        "settings":
                                        {
                                            "directions" : ["X","Y"],
                                            "balanced_tree" : false,
                                            "parallel": true
                                        }
                                    }
                                ]
                            }
                        }
                    }
                }
            }
        ]
    }
}